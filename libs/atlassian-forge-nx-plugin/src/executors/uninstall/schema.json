{
  "$schema": "http://json-schema.org/schema",
  "version": 2,
  "title": "Uninstall executor",
  "description": "uninstall the app from an Atlassian site",
  "type": "object",
  "properties": {
    "installationId": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "verbose": {
      "type": "boolean",
      "description": "enable verbose mode",
      "default": false
    },
    "help": {
      "alias": "h",
      "type": "boolean",
      "description": "display help for command",
      "default": false
    }
  },
  "required": []
}
